{
  "name": "test-search",
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "api/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "./public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/app.js"
    },
    {
      "src": "/:match*",
      "dest": "public/:match*"
    }
  ]
}