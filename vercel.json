{
  "name": "test-search",
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "api/app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/app.js",
      "headers": {
        "cache-control": "public, max-age=3600"
      }
    }
  ]
}