{
  "name": "test-search",
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "api/app.js",
      "use": "@vercel/node"
    },
    {
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/app.js"
    }
  ]
}